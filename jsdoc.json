{
    "source": {
        "include": [
            "src"
        ],
        "includePattern": ".+\\.(js|jsx)$",
        "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
        "destination": "./docs",
        "recurse": true,
        "readme": "./README.md"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}